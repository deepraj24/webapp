node ('docker-client') {
    
      stage ('Code from SCM') {
        git credentialsId: 'github-demo-1', url: 'https://github.com/deepraj24/webapp'
      }
      stage ('Build') {
        sh '''mvn clean install'''
      }
      stage ('deploy to docker container')
    {
        sh '''docker build -t app:1 .
        docker run -d -p 8080:8080 --name web_app app:1'''
    }
}
